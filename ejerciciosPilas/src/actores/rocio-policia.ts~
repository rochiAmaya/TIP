/// <reference path="ActorAnimado.ts"/>
class Policia extends ActorAnimado{
	
	ubicacionColumna;
	ubicacionFila;

 	constructor(x, y) {
		super(20, 20, {grilla: 'pelota.png', cantColumnas: 1});
		this.ubicacionFila = x;
		this.ubicacionColumna = y;
	    }

	meDejaPasar(){
		return true;//(new Random(2))==1;
	}

}

